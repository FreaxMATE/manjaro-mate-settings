<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="FreaxMATE" _name="FreaxMATE" version="1.0">
  <author>Konstantin Unruh</author>
  <_description>A color scheme matching GTK's default theme</_description>

  <!-- For Builder -->
  <style name="debugger::breakpoint"        background="selection_bg" foreground="#ffffff"/>

  <!-- Global Settings -->
  <style name="text"                        foreground="#B5A883" background="#1A1A1A"/>
  <style name="map-overlay"                 background="#rgba(46,52,54,.3)"/>
  <style name="cursor"                      foreground="#8F1E2C"/>
  <style name="line-numbers"                foreground="#59534E" background="#1A1A1A"/>
  <style name="current-line"                background="#191E29"/>
  <style name="current-line-number"         background="#A79B8E" />
  <style name="right-margin"                foreground="#59534E" background="#1A1A1A"/>
  <style name="draw-spaces"                 foreground="#B5A883"/>
  <style name="background-pattern"          background="#59534E"/>

  <style name="bracket-match"               background="#405730" bold="true"/>
  <style name="bracket-mismatch"            foreground="#1A1A1A" background="#6D3B6D" bold="true"/>
  <style name="search-match"                foreground="#89BBBC" background="#336190"/>
  <style name="misspelled-match"            foreground="#89BBBC" background="#336190"/>

  <style name="def:base-n-integer"          foreground="#B5A883"/>
  <style name="def:boolean"                 foreground="#A82045"/>
  <style name="def:comment"                 foreground="#6F3081"/>
  <style name="def:constant"                foreground="#B5A883"/>
  <style name="def:decimal"                 foreground="#B5A883"/>
  <style name="def:deletion"                strikethrough="true"/>
  <style name="def:doc-comment-element"     italic="true"/>
  <style name="def:emphasis"                italic="true"/>
  <style name="def:error"                   line-background="#A82045" />
  <style name="def:floating-point"          foreground="#B5A883"/>
  <style name="def:function"                foreground="#E1B55C" bold="true"/>
  <style name="def:heading"                 foreground="#B5A883" bold="true"/>
  <style name="def:identifier"              foreground="#83A89C"/>
  <style name="def:inline-code"             foreground="#B5A883"/>
  <style name="def:insertion"               underline="single"/>
  <style name="def:keyword"                 foreground="#A82045" />
  <style name="def:link-destination"        italic="true" underline="true"/>
  <style name="def:link-symbol"             foreground="#308181" bold="true"/>
  <style name="def:link-text"               foreground="#308181"/>
  <style name="def:list-marker"             foreground="#308181" bold="true"/>
  <style name="def:net-address"             italic="true" underline="true"/>
  <style name="def:note"                    foreground="#E1B55C" background="#1A1A1A" bold="true"/>
  <style name="def:preformatting-section"   foreground="#B5A883"/>
  <style name="def:preprocessor"            foreground="#A82045"/>
  <style name="def:shebang"                 foreground="#C11345" underline="true"/>
  <style name="def:special-char"            foreground="#B5A883"/>
  <style name="def:special-constant"        foreground="#B5A883"/>
  <style name="def:statement"               foreground="#B5A883" bold="true"/>
  <style name="def:string"                  foreground="#9CB545"/>
  <style name="def:strong-emphasis"         foreground="#B5A883" bold="true"/>
  <style name="def:thematic-break"          foreground="#B5A883" bold="true"/>
  <style name="def:type"                    foreground="#B5A883" bold="true"/>
  <style name="def:warning"                 background="#B5A883"/>

</style-scheme>
